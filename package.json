{
  "name": "graphql-redis-ts-psql-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "dotenv -e ./dotenv/dev.env -- ts-node-dev --no-notify --respawn --transpile-only src/index.ts",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "studio": "prisma studio --experimental",
    "migrate:save": "dotenv -e ./dotenv/prod.env -- prisma migrate save --experimental",
    "migrate:up": "dotenv -e ./dotenv/prod.env -- prisma migrate up --experimental",
    "lint": "eslint src/**/**/*.ts && eslint tests/**/**/*.ts",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "clean": "rm -rf dist"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nexus/schema": "^0.17.0",
    "@prisma/cli": "^2.10.2",
    "@prisma/client": "2.10.0",
    "@types/bcrypt": "^3.0.0",
    "@types/node": "^14.14.6",
    "apollo": "^2.31.1",
    "apollo-server": "^2.19.0",
    "apollo-server-core": "^2.19.0",
    "apollo-server-express": "^2.19.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.15.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.4.0",
    "jsonwebtoken": "^8.5.1",
    "nexus-plugin-prisma": "^0.23.1",
    "server": "^1.0.31"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  }
}
